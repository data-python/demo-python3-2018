   "source": [
    "from collections import Counter\n",
    "\n",
    "def find_all_duplicates(lst):\n",
    "    c = Counter(lst)\n",
    "    return list(filter(lambda k: c[k] > 1, c))\n",
    "\n",
    "\n",
    "print(find_all_duplicates([1, 2, 2, 3, 3, 3]))  # [2,3]\n",
    "\n"
   ]
  }
